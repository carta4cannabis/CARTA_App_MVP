{
  "expo": {
    "name": "CARTA",
    "slug": "carta",
    "jsEngine": "jsc",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#061A18"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "CARTA needs camera access to scan product QR codes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#061A18"
      },
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-barcode-scanner",
      "expo-sqlite",
      "expo-font",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "cde47772-74a1-4c9f-9c4d-67fd0d65461f"
      }
    }
  }
}
