{
  "expo": {
    "name": "CARTA Stack",
    "slug": "carta",
    "jsEngine": "hermes",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#061A18"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/cde47772-74a1-4c9f-9c4d-67fd0d65461f"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "9",
      "infoPlist": {
        "NSCameraUsageDescription": "CARTA needs camera access to scan product QR codes.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "carta4cannabis"
    },
    "android": {
      "versionCode": 101,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#061A18"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.carta4cannabis.carta"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-sqlite",
      "expo-font",
      "expo-asset",
      "expo-camera"
    ],
    "extra": {
      "eas": {
        "projectId": "cde47772-74a1-4c9f-9c4d-67fd0d65461f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
