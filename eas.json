{
  "cli": {
    "version": ">= 13.0.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EAS_NO_FROZEN_LOCKFILE": "1"
      },
      "channel": "development"
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EAS_NO_FROZEN_LOCKFILE": "1"
      },
      "channel": "production"
    }
  }
}
